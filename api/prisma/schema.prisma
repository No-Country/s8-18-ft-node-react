datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URI_DEV")
}

generator client {
  provider = "prisma-client-js"
}

model Users {
  id           String   @id @default(uuid()) @db.VarChar(36)
  first_name   String
  last_name    String
  email        String   @unique
  password     String
  phone_number Int
  createdAt    DateTime @default(now())
  updateAt     DateTime @updatedAt
}
